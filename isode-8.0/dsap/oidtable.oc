	mHS-O:

mHSOrganisationUnit:mhs-ds-tree-oc.6:\
	oRAddressComponent:\
	mHS-OU:

mHSPerson: mhs-ds-tree-oc.7:oRAddressComponent:cn:

mHSNamedObject:mhs-ds-tree-oc.4:oRAddressComponent:\
	mhsCN:

