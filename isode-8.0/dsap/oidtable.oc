dNSDomain:	 pilotObjectClass.15 : domain : : dNSRecord

domainRelatedObject:	 pilotObjectClass.17 : top : associatedDomain : 

friendlyCountry:	 quipuObjectClass.3, pilotObjectClass.18 : \
